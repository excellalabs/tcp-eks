<?xml version='1.1' encoding='UTF-8'?>
<github-plugin-configuration plugin="github@1.29.2">
  <configs>
    <github-server-config>
      <name>github</name>
      <apiUrl>https://api.github.com</apiUrl>
      <manageHooks>true</manageHooks>
      <credentialsId><%= @github_token_id %></credentialsId>
      <clientCacheSize>20</clientCacheSize>
    </github-server-config>
  </configs>
  <hookSecretConfig>
    <credentialsId></credentialsId>
  </hookSecretConfig>
</github-plugin-configuration>